    <RootNamespace>Ghosts_and_Gravestones</RootNamespace> 
    <WarnOn>3390;$(WarnOn)</WarnOn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="LocalResourceManager.fs" />
    <Compile Include="GameMenu.fs" />
    <Compile Include="CellState.fs" />
    <Compile Include="CellArgs.fs" />
    <Compile Include="Cell.fs" />
    <Compile Include="BoardArgs.fs" />
    <Compile Include="Breadcrumb.fs" />
    <Compile Include="Board.fs" />
